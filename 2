getgenv().main = {
    osirisEnabled = true,
    FovMode = 2, -- 2 STICKS FOV TO BODYPART; 1 STICKS FOV TO CURSOR;
    Prediction = 0.10421, --self explanatory
    Key = "C", --target lock key
    UnlockKey = "Z",
    AutoPrediction = false, -- will overwrite Prediction value and use a DECENT prediction for your ping
    ExecuteLayAnimOnKeybind = false,
    LayKeybind = "X",
    NoBulletDelay = true,
    targetWallCheck = false, -- set to true if u wanna target ppl who are inside buildings
    fpsBoost = false,
    SafeSilent = true,
    AntiFloorShots = true,
    HideVisualsKeybind = "K",
    HideVisualsOnStart = true,
    PredictionVisualizer = false -- FOCUS ON TRACING THE RED DOT THIS IS THE PLACE WHERE THE SHOT LOOKS THE MOST LEGIT ON CLIP

}

