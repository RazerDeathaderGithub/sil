getgenv().SmoothCamlock = { --This is fully streamable, there's no GUI; THIS SCRIPT IS NOT OWNED OR MAINTAINED BY ME
    LoadCamLock = false,
    ThirdPerson = true,
    FirstPerson = true,
    TeamCheck = false, -- Check if Target is on your Team (True means it wont lock onto your teamates, false is vice versa) (Set it to false if there are no teams)
    PredictMovement = true, -- Predicts if they are moving in fast velocity (like jumping) so the aimbot will go a bit faster to match their speed 
    Prediction = 0.119,
    CheckIfJumped = true,
    Smoothness = true,
    SmoothnessAmount = 0.022,

    camlockFOV = 45 -- SET TO FALSE IF YOU WANT IT TO BE THE SAME AS GUN FOVS
}

